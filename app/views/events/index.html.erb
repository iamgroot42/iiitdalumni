<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>End</th>
      <th>Title</th>
      <th>Summary</th>
      <th>Description</th>
      <th>User</th>
      <th>Contact</th>
      <th>Venue</th>
      <th>Fblink</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= event.title %></td>
        <td><%= event.summary %></td>
        <td><%= event.description %></td>
        <td><%= event.user %></td>
        <td><%= event.contact %></td>
        <td><%= event.venue %></td>
        <td><%= event.fblink %></td>
        <td><%= event.photo %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
